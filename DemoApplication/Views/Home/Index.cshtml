@{
    ViewData["Title"] = "Home Page";
}

<html>
<head>
    <link href="~/css/Index.css" rel="stylesheet" />
     <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
     <script type="text/javascript">
         $(document).ready(function () {
             $('#login').click(function () {
                 window.location.href = "/Home/Login";
             })
         })
     </script>
</head>


<body>
    <div class="panel panel-primary" id="mypanel" style="height:350px;height:350px;">
        <div class="panel-header">
            <label class="text-secondary">NorthWest Missouri State University</label>
        </div>
     
        

    </div>
    <div class="panel panel-primary">
        <div class="panel-body">
            <div class="panel-body" style="padding-right:50px;">
                <div class="btn-group">
                    <input class="btn btn-primary btn-lg" value="Login" type="submit" style="padding-right:150px;margin-left:820px;" id="login" />
                </div>
                <br /><br />
                <div class="btn-group">
                    <input class="btn btn-primary btn-lg" value="Register" type="submit" style="padding-right:150px;margin-left:820px;" />
                </div>
            </div>
        </div>
    </div>
   
</body>
</html>
